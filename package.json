{
  "name": "gh-package-nuxt-module",
  "version": "1.0.9",
  "description": "My new Nuxt module",
  "license": "MIT",
  "type": "module",
  "moduleResolution": "node",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Nuxt-Packages/gh-package-nuxt-module.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-nightly@latest"
  },
  "devDependencies": {
    "@types/node": "^20.11.29",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.11.1",
    "@nuxt/test-utils": "^3.12.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.57.0",
    "nuxt": "^3.11.1",
    "vitest": "^1.4.0"
  },
  "publishConfig": {
    "@Compro-Rishabh:registry": "https://npm.pkg.github.com"
  },
  "bugs": {
    "url": "https://github.com/Nuxt-Packages/gh-package-nuxt-module/issues"
  },
  "homepage": "https://github.com/Nuxt-Packages/gh-package-nuxt-module#readme",
  "directories": {
    "test": "test"
  },
  "author": ""
}
